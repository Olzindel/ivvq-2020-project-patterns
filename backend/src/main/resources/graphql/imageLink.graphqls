type ImageLink {
    id: ID!
    imageLink: String!
    product: Product!
}

extend type Query {
    imageLinks(count: Int!):[ImageLink]
    imageLink(imageLinkId: ID!):ImageLink
}

extend type Mutation {
    updateImageLink(imageLinkId:ID!, imageLink:String!, productId:ID!):ImageLink
    createImageLink(imageLink:String!, productId:ID!):ImageLink
    deleteImageLink(imageLinkId:ID!):ID
}
