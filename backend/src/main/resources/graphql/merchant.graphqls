type Merchant {
    id: ID!
    name: String!
    admin: User!
    products: [Product!]
}

input MerchantInput {
    name:String
    userId:ID
    productIds:[ID!]
}

extend type Query {
    merchants(count: Int!):[Merchant]
    merchant(merchantId: ID!):Merchant
}

extend type Mutation {
    updateMerchant(merchantId:ID!, input:MerchantInput!):Merchant
    createMerchant(input:MerchantInput):Merchant
    deleteMerchant(merchantId:ID!):ID
}