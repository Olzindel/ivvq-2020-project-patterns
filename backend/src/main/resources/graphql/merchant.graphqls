type Merchant {
    id: ID!
    name: String!
    createdAt: String
    admin: User!
}

input MerchantInput {
    id: ID!
    name: String
    createdAt: String
    admin: UserInput
}

extend type Query {
    merchants(count: Int!):[Merchant]
    merchant(merchantId: ID!):Merchant
}

extend type Mutation {
    updateMerchant(input:MerchantInput):Merchant
    createMerchant(name:String! , userId:ID!, createdAt:String):Merchant
    deleteMerchant(merchantId:ID!):ID
}