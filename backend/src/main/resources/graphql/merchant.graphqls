type Merchant {
    id: ID!
    name: String!
    createdAt: String
    admin: User!
    products: [Product!]
}

extend type Query {
    merchants(count: Int!):[Merchant]
    merchant(merchantId: ID!):Merchant
}

extend type Mutation {
    updateMerchant(merchantId:ID!, name:String , userId:ID, createdAt:String, productIds:[ID!]):Merchant
    createMerchant(name:String! , userId:ID!, productIds:[ID!]):Merchant
    deleteMerchant(merchantId:ID!):ID
}