type Order {
    id: ID!,
    createdAt: String!,
    orderStatus: OrderStatus!,
    OrderItems: [OrderItem!]!,
    User: User!
}

enum OrderStatus {
    BASKET
    PAID
    ABORTED
}

extend type Query {
    orders(count: Int!):[Order]
    order(orderId: ID!):Order
}

extend type Mutation {
    createOrder(orderStatus:OrderStatus!,orderItemIds:[ID!]!, userId:ID! ):Order
    deleteOrder(orderId:ID!):ID
}