type Product {
    id: ID!,
    name: String!,
    price: Float!,
    status: String!,
    createdAt: String,
    imageLink: String,
    merchant: Merchant!
}


extend type Query {
    products(count: Int!):[Product]
    product(productId: ID!):Product
}

extend type Mutation {
    createProduct(name:String!, price:Float!, status:String!, imageLink:String, merchantId:ID!):Product
    deleteProduct(productId:ID!):ID
}